{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"moduleResolution": "bundler",
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", ".svelte-kit", "build"]
}
